using System;
public class Program 
{
        public static void Main()
        {
                int age = 18;
                int ageMinimum = 0;
                int ageMaximum = 99;

                ageMinimum = age;
                age = age + 20; 
                ageMaximum = age;
                ageMinimum = ageMaximum - 30;
        }

}